<li 
  class="card branded"
  style="
    {% if link.data.theme %}
      --card-background: {{ link.data.theme.primary }};
      --card-background-shadow: {{ link.data.theme.shadow }};
    {% endif %}
  "
>
  <a href="{{ link.data.url }}">
    <span>{{ link.data.label }}</span>
    <span>{{ link.data.username or link.data.url }}</span>
  </a>
</li>